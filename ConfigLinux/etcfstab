proc            /proc           proc    defaults          0       0
PARTUUID=bfd4e594-01  /boot           vfat    defaults,flush    0       2
PARTUUID=bfd4e594-02  /               ext4    defaults,noatime  0       1
# a swapfile is not a swap partition, no line here
#   use  dphys-swapfile swap[on|off]  for that
tmpfs /media/ramdiskS63/ tmpfs defaults,size=30M 0 0
